{
  "node": true,
  "esnext": true,
  "bitwise": false,
  "camelcase": true,
  "eqeqeq": true,
  "eqnull": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "nonew": true,
  "noarg": true,
  "quotmark": "double",
  "undef": true,
  "unused": false,
  "trailing": true,
  "sub": true,
  "curly": true,
  "boss": true,
  "expr": true,
  "predef": [
    "console"
  ],
  "globals": {
    "Promise": true
  },
  "strict": "global"
}
